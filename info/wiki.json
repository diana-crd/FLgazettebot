            "1": {
                "value": "Reptiles",
                "alias": "a Reptile",
                "animal_blurb": "The crowd at the Bone Market is simply roaring",
                "animal_bonus": "10",
                "animal_conditions": "have a non-human ribcage, one skull, one tail, no wings or fins, and either no arms and at most four legs, or two arms and two legs. You must also possess [[A Complete Account of Frogs, Toads, and Other Croaking Beasts]]"
            },
            "2": {
                "value": "Amphibians",
                "alias": "an Amphibian",
                "animal_blurb": "The Bone Market croaks with activity",
                "animal_bonus": "10",
                "animal_conditions": "have a non-human ribcage, one skull, four legs, and no tails, arms, wings, or fins. You must also possess [[A Complete Account of Frogs, Toads, and Other Croaking Beasts]]"
            },
            "3": {
                "value": "Birds",
                "alias": "a Bird",
                "animal_blurb": "It seems everyone has been dreaming feathers",
                "animal_bonus": "10",
                "animal_conditions": "have a non-human ribcage, at least one skull, two wings, two legs, at most one tail, and no arms or fins. You must also possess a [[Comprehensive Study of Avian Anatomies, Oneiric and Otherwise]]"
            },
            "4": {
                "value": "Fish",
                "alias": "a Fish",
                "animal_blurb": "Zailors swarm the Bone Market",
                "animal_bonus": "15",
                "animal_conditions": "have a non-human ribcage, one skull, at least two fins, at most one tail, and no arms, wings, or legs. You must also possess an [[Unexpurgated Accounting of the Anatomies of Aquatic Life-forms]]"
            },
            "5": {
                "value": "Arachnids",
                "alias": "an Arachnid",
                "animal_bonus": "15",
                "animal_blurb": "Buyers are really eyeing the stalls",
                "animal_conditions": "have a non-human ribcage, eight legs, at most one tail, and no heads, arms, wings, or fins. You must also possess a [[Survey of Arachnids and Insects Native to the Neath and Unterzee]]"
            },
            "6": {
                "value": "Insects",
                "alias": "an Insect",
                "animal_blurb": "The Bone Market is crawling with buyers",
                "animal_bonus": "15",
                "animal_conditions": "have a non-human ribcage, one head, six legs, at most four wings, and no tails, arms, or fins. You must also possess a [[Survey of Arachnids and Insects Native to the Neath and Unterzee]]"
            },
            "7": {
                "value": "Primates",
                "alias": "one of various types of Primate",
                "animal_blurb": "The Bone Market turns introspective!",
                "animal_bonus": "10",
                "animal_conditions": "have a humanoid ribcage, one head, and either: two arms, two legs, and no wings, fins, or tails (Humanoid); four arms and no legs, wings, fins, or tails (Ape); four arms, one tail, and no legs, wings, or fins (Monkey)"
            }
        }
    },
